@using WebTatilSitesi.Models.Classes
@model List<BlogSinifi>

<div class="padding1">
    <h4>Favori Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogGorsel" style="width:120px; height:65px;" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,70)...</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    <partial name="~/Views/Shared/_Partial5.cshtml">
</div>